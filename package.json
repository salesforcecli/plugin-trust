{
  "name": "@salesforce/plugin-trust",
  "description": "validate a digital signature for a npm package",
  "version": "2.2.2",
  "author": "Salesforce",
  "main": "lib/index.js",
  "bin": {
    "sf-trust": "bin/dev"
  },
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^1.21.0",
    "@salesforce/core": "^3.32.9",
    "@salesforce/sf-plugins-core": "^1.20.0",
    "got": "^11",
    "npm": "^8.12.1",
    "npm-run-path": "^4.0.1",
    "proxy-agent": "^5.0.0",
    "proxy-from-env": "^1.1.0",
    "shelljs": "^0.8.4",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.2.11",
    "@salesforce/cli-plugins-testkit": "^3.2.0",
    "@salesforce/dev-config": "^3.0.0",
    "@salesforce/dev-scripts": "^3.1.0",
    "@salesforce/plugin-command-reference": "^2.2.8",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.4.2",
    "@swc/core": "^1.3.14",
    "@types/proxy-from-env": "^1.0.1",
    "@types/shelljs": "^0.8.9",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.45.1",
    "chai": "^4.2.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-sf-plugin": "^1.1.5",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "oclif": "^3.2.28",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.0",
    "shx": "0.3.4",
    "sinon": "10.0.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "config": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin",
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-trust",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "plugins:preinstall:verify:signature": [
        "./lib/hooks/verifyInstallSignature.js"
      ]
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "plugins": {
        "description": "list installed plugins",
        "subtopics": {
          "trust": {
            "description": "validate a digital signature for a npm package"
          }
        }
      }
    }
  },
  "repository": "salesforcecli/plugin-trust",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  }
}